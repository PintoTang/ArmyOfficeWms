<Window x:Class="WHSE.Monitor.Framework.UserControls.SettingForm"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:WHSE.Monitor.Framework.UserControls"
		mc:Ignorable="d" 
		Title="SettingForm" Height="450" Width="800">
	<Window.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="CommonStyle\CommStyle.xaml"></ResourceDictionary>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Window.Resources>
	<Grid>
		<StackPanel >
			<Expander Header="Expander" HorizontalAlignment="Left" Height="145" VerticalAlignment="Top" Width="792" Background="{StaticResource ExpanelHeaderBg}">
				<Grid Background="{StaticResource ExpanelContentBg}"/>
			</Expander>

			<Expander Header="Expander" HorizontalAlignment="Left" Height="145" VerticalAlignment="Top" Width="792" Background="{StaticResource ExpanelHeaderBg}">
				<Grid Background="{StaticResource ExpanelContentBg}"/>
			</Expander>


		</StackPanel>
		
	</Grid>
</Window>
